  "dependencies": {
    "fs": "^0.0.1-security",
    "playwright": "^1.24.2",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.24.2",
    "allure-playwright": "^2.0.0-beta.16"
